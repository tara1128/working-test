{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    ["transform-class-properties"],
    ["transform-es2015-classes", { "loose": true }],
    ["add-module-exports"],
    ["transform-runtime"],
    ["antd"]
  ],
  "env": {
    "development": {
    },
    "production": {
      "optional": ["optimisation", "minification"],
      "presets": ["react-hmre"]
    }
  }
}
